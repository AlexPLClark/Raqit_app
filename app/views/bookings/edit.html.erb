<div class="offset-sm-3 col-sm-6">
  <div class="offset-sm-3 col-sm-6">
    <h1 class="large-h1-white">Edit your booking</h1>
  </div>
  <div class="offset-sm-3 col-sm-6 form">
    <%= simple_form_for [@racket, @booking] do |f| %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.submit "Edit booking", class: "btn-primary-v-primary-af" %>
  <% end %>
</div>
