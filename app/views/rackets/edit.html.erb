<h1>Editing Racket</h1>

<div class="offset-sm-3 col-sm-6">
  <%= simple_form_for @racket do |f| %>
    <%= f.input :name %>
    <%= f.input :location %>
    <%= f.input :description %>
    <%= f.input :brand %>
    <%= f.input :image_url %>
    <% unless @racket.new_record? %>
      <%= f.input :name %>
    <% end %>
    <%= f.submit %>
  <% end %>
</div>

<%= link_to 'Back', rackets_path %>