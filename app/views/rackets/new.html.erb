<h1>Adding a racket</h1>
<div class="offset-sm-3 col-sm-6">
  <%= simple_form_for @racket do |f| %>
    <%= f.input :name %>
    <%= f.input :location %>
    <%= f.input :description %>
    <%= f.input :brand %>
    <%= f.input :photo, as: :file %>
    <% unless @racket.new_record? %>
      <%= f.input :name %>
    <% end %>
    <%= f.submit %>
  <% end %>

</div>
